@model IEnumerable<ApplicationCore.Entities.Cast>
  
    <table class="table">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Character
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Character)
                </td>
            </tr>
            }
        </tbody>
    </table>
